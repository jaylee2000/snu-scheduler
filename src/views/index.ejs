<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %>!</p>
	  
	 
	 <% for(let schedule of allSchedules) { %>
	  <div>
	  	<%= schedule.subjectName %><br>
	  	mon: <%= schedule.mon.start %> ~ <%= schedule.mon.end %><br>
	  	tue: <%= schedule.tue.start %> ~ <%= schedule.tue.end %><br>
	  	wed: <%= schedule.wed.start %> ~ <%= schedule.wed.end %><br>
	  	thur: <%= schedule.thur.start %> ~ <%= schedule.thur.end %><br>
	  	fri: <%= schedule.fri.start %> ~ <%= schedule.fri.end %><br>
		  
	  	Weight: <%= schedule.weight %>
		  <form action="/<%= schedule._id %>/?_method=DELETE" method="POST">
			  <button>Delete</button>
		  </form>
		  <form action="/update/<%= schedule._id %>">
			  <button>Update</button>
		  </form>
	  </div>
		<br>
	 <% } %>
	  
	 <a href="/new">Add new custom schedule</a>
	  <br>
	 <a href="/best">View optimized schedule</a>
	  
  </body>
</html>
